{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "AnswerCode.Services.LLMRequestLoggingHandler": "Warning"
    }
  },
  "AllowedHosts": "*",
  "QASourceCodePath": {
    "DefaultPath": "--the path of your source code for QA--"
  },
  "LLM": {
    "DefaultProvider": "OpenAI",
    "Providers": {
      "OpenAI": {
        "Endpoint": "---your endpoint---",
        "ApiKey": "---your api key---",
        "Model": "---your model---",
        "DisplayName": "---your display name---"
      },
      "Ollama": {
        "Endpoint": "http://localhost:11434/v1",
        "ApiKey": "ollama",
        "Model": "---your ollama model name, e.g. llama3.2 or qwen2.5---",
        "DisplayName": "---your display name---"
      }
    }
  }
}